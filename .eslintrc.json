{
  "parser": "@typescript-eslint/parser",
  "extends": ["plugin:@typescript-eslint/recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["simple-import-sort"],
  "rules": {
    "no-unused-vars": "error",
    "@typescript-eslint/no-explicit-any": "off"
  }
}
